  "dependencies": {
    "@remap/authenticate-middleware": "^1.0.2",
    "apollo-server-micro": "^2.1.0",
    "graphql": "^14.0.2",
    "micro": "^9.3.3",
    "micro-compose": "^0.0.3",
    "micro-cors-multiple-allow-origin": "^1.0.1",
    "micro-errors": "^2.0.0",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.0",
